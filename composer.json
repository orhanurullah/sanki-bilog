{
    "name": "sankii/bilog",
    "description": "A blog package for laravel",
    "keywords": [
        "sankii",
        "bilog"
    ],
    "homepage": "https://github.com/sankii/bilog",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Orhan Nurullah",
            "email": "orhann.1512@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Sankii\\Bilog\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sankii\\Bilog\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sankii\\Bilog\\BilogServiceProvider"
            ],
            "aliases": {
                "Bilog": "Sankii\\Bilog\\BilogFacade"
            }
        }
    }
}
